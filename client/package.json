{
  "name": "@trshcmpctr/client",
  "version": "1.0.0",
  "description": "Frontend React app",
  "files": [
    "dist",
    "lib/paths.js"
  ],
  "type": "module",
  "main": "./dist/manifest.json",
  "module": "./dist/manifest.json",
  "exports": {
    ".": "./dist/manifest.json",
    "./paths": "./lib/paths.js"
  },
  "scripts": {
    "build": "webpack --config webpack.config.cjs",
    "build:production": "webpack --config webpack.config.cjs --env production",
    "build:watch-webpack": "nodemon --exec 'webpack --config webpack.config.cjs' --watch webpack.config.cjs --watch babel.config.cjs --watch babel.config.lib.cjs",
    "clean": "rm -rf dist",
    "cy:open": "cypress open --config-file=cypress/cypress.config.ts --e2e --browser=chrome",
    "cy:run": "cypress run --config-file=cypress/cypress.config.ts",
    "lint": "eslint --report-unused-disable-directives --ext .js,.jsx,.ts,.tsx,.cjs --max-warnings=0 .",
    "lint:md": "markdownlint-cli2 --config node_modules/@trshcmpctr/markdownlint-config/.markdownlint-cli2.jsonc",
    "serve": "webpack serve --config webpack.config.cjs",
    "serve:watch-webpack": "nodemon --exec 'webpack serve --config webpack.config.cjs' --watch webpack.config.cjs --watch babel.config.cjs --watch babel.config.lib.cjs",
    "start": "webpack serve --config webpack.config.cjs --open",
    "start:production": "webpack serve --config webpack.config.cjs --open --env production",
    "test": "jest --reporters=@trshcmpctr/jest-stdout-reporter --silent",
    "test:debug": "NODE_OPTIONS=--inspect jest --runInBand --no-cache",
    "test:watch": "jest --watch",
    "test:cypress": "start-server-and-test serve http://localhost:8080 cy:run",
    "type-check": "tsc",
    "watch": "webpack --config webpack.config.cjs --watch"
  },
  "contributors": [
    {
      "email": "shanedgarrity@gmail.com",
      "name": "Shane Garrity"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shanedg/trshcmpctr.git",
    "directory": "client"
  },
  "homepage": "https://github.com/shanedg/trshcmpctr/tree/main/client#readme",
  "private": true,
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "~7.26.0",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/plugin-transform-modules-commonjs": "~7.25.9",
    "@babel/preset-env": "~7.26.0",
    "@babel/preset-react": "~7.25.9",
    "@babel/preset-typescript": "~7.26.0",
    "@testing-library/dom": "~10.4.0",
    "@testing-library/jest-dom": "~6.6.3",
    "@testing-library/react": "~16.0.0",
    "@trshcmpctr/eslint-config-jest": "workspace:*",
    "@trshcmpctr/eslint-config-react": "workspace:*",
    "@trshcmpctr/eslint-config-typescript": "workspace:*",
    "@trshcmpctr/eslint-config": "workspace:*",
    "@trshcmpctr/jest-stdout-reporter": "workspace:*",
    "@trshcmpctr/markdownlint-config": "workspace:*",
    "@types/jest": "~29.5.2",
    "@types/node": "~18.19.31",
    "@types/prop-types": "~15.7.12",
    "@types/react-dom": "~18.3.0",
    "@types/react": "~18.3.3",
    "@typescript-eslint/eslint-plugin": "~8.16.0",
    "@typescript-eslint/parser": "~8.16.0",
    "babel-jest": "~29.7.0",
    "babel-loader": "~9.2.1",
    "babel-plugin-syntax-dynamic-import": "~6.18.0",
    "css-loader": "~7.1.1",
    "cypress": "13.16.0",
    "discord-api-types": "~0.37.46",
    "eslint-import-resolver-typescript": "~3.6.1",
    "eslint-plugin-cypress": "~4.1.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.31.0",
    "eslint-plugin-jest": "~28.9.0",
    "eslint-plugin-jsx-a11y": "~6.10.2",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-react-hooks": "~5.0.0",
    "eslint-plugin-react": "~7.37.2",
    "eslint-plugin-testing-library": "~7.0.0",
    "eslint-webpack-plugin": "~4.2.0",
    "eslint": "~9.16.0",
    "html-webpack-plugin": "~5.6.0",
    "jest-environment-jsdom": "~29.7.0",
    "jest-serializer-path": "~0.1.15",
    "jest": "~29.7.0",
    "markdownlint-cli2": "~0.15.0",
    "nodemon": "~3.1.0",
    "start-server-and-test": "~2.0.0",
    "style-loader": "~4.0.0",
    "terser-webpack-plugin": "~5.3.1",
    "typescript": "~5.7.2",
    "webpack-cli": "~5.1.4",
    "webpack-dev-server": "~5.1.0",
    "webpack-manifest-plugin": "~5.0.0",
    "webpack": "~5.96.1"
  },
  "dependencies": {
    "axios": "~1.7.2",
    "core-js": "~3.39.0",
    "prop-types": "~15.8.1",
    "react-dom": "~18.3.1",
    "react-router": "~7.0.1",
    "react": "~18.3.1"
  }
}
