{
  "name": "@trshcmpctr/eslint-config-typescript",
  "version": "1.0.0",
  "description": "ESLint config for TypeScript in JavaScript projects.",
  "main": "lib/eslint-config-typescript.js",
  "scripts": {
    "build": "echo fake build || true",
    "clean": "rimraf *.build.log",
    "lint": "eslint --max-warnings=0 --fix --cache --cache-location node_modules/.cache/eslint-cache/ .",
    "lint:md": "markdownlint-cli2-config 'node_modules/@trshcmpctr/markdownlint-config/.markdownlint-cli2.jsonc' '**/*.md' '#node_modules'"
  },
  "devDependencies": {
    "@trshcmpctr/eslint-config": "workspace:*",
    "@trshcmpctr/markdownlint-config": "workspace:*",
    "@typescript-eslint/eslint-plugin": "~5.13.0",
    "@typescript-eslint/parser": "~5.13.0",
    "eslint-import-resolver-typescript": "~2.5.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.25.4",
    "eslint-plugin-node": "~11.1.0",
    "eslint": "^8.7.0",
    "markdownlint-cli2": "~0.4.0",
    "rimraf": "~3.0.2",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "~5.13.0",
    "@typescript-eslint/eslint-plugin": "~5.13.0",
    "eslint-import-resolver-typescript": "~2.5.0",
    "eslint-plugin-import": "~2.25.4",
    "eslint": "^7.x.x || ^8.x.x",
    "eslint-plugin-jest": "~26.1.1",
    "typescript": "^4.x.x"
  }
}
