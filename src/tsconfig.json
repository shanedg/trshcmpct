{
  "extends": "../tsconfig.json",
  "include": [
    "*.ts",
    "*.tsx",
    "*.js",
    "*.jsx"
  ],
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": false,
    "strict": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "jsx": "preserve",
    "composite": true,
    "declaration": true
  }
}
