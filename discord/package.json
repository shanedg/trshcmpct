{
  "name": "@trshcmpctr/discord",
  "version": "1.0.0",
  "description": "discord",
  "files": [
    "lib",
    "!lib/**/*.test.*",
    "!lib/**/*.ava-test.*"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module",
  "contributors": [
    {
      "email": "shanedgarrity@gmail.com",
      "name": "Shane Garrity"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shanedg/trshcmpctr.git",
    "directory": "discord"
  },
  "homepage": "https://github.com/shanedg/trshcmpctr/tree/main/discord#readme",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "echo fake build || true",
    "clean": "rimraf *.build.log",
    "debug": "node --experimental-specifier-resolution=node --experimental-json-modules --trace-warnings --inspect-brk lib/index.js",
    "dev": "nodemon --experimental-specifier-resolution=node --experimental-json-modules --trace-warnings --inspect lib/index.js",
    "lint": "eslint --max-warnings=0 .",
    "lint:md": "markdownlint-cli2-config 'node_modules/@trshcmpctr/markdownlint-config/.markdownlint-cli2.jsonc' '**/*.md' '#node_modules'",
    "start": "node --experimental-specifier-resolution=node --experimental-json-modules lib/index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --no-cache --silent",
    "test:ava": "NODE_OPTIONS=--experimental-specifier-resolution=node ava"
  },
  "jest": {
    "reporters": ["@trshcmpctr/jest-stdout-reporter"]
  },
  "ava": {
    "cache": false,
    "files": ["**/*.ava-test.*"]
  },
  "devDependencies": {
    "@jest/globals": "~27.5.1",
    "@tophat/eslint-import-resolver-require": "0.1.3",
    "@trshcmpctr/eslint-config-jest": "workspace:*",
    "@trshcmpctr/eslint-config": "workspace:*",
    "@trshcmpctr/jest-stdout-reporter": "workspace:*",
    "@trshcmpctr/markdownlint-config": "workspace:*",
    "ava": "~4.2.0",
    "eslint-plugin-ava": "~13.2.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.25.4",
    "eslint-plugin-jest": "~26.1.1",
    "eslint-plugin-node": "~11.1.0",
    "eslint": "^8.7.0",
    "jest": "^27.4.7",
    "markdownlint-cli2": "~0.4.0",
    "nodemon": "~2.0.15",
    "rimraf": "~3.0.2",
    "sinon": "~13.0.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "cookie-session": "~2.0.0",
    "express": "~4.17.3",
    "hbs": "~4.2.0",
    "node-fetch": "~2.6.7",
    "pino-http": "~6.6.0"
  }
}
