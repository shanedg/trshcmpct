{
  "name": "@trshcmpctr/eslint-config-jest",
  "version": "1.0.1",
  "description": "ESLint config for Jest tests in JavaScript projects.",
  "main": "lib/eslint-config-jest.js",
  "files": ["lib/eslint-config-jest.js"],
  "contributors": [
    {
      "email": "shanedgarrity@gmail.com",
      "name": "Shane Garrity"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shanedg/trshcmpctr.git",
    "directory": "eslint-config-jest"
  },
  "homepage": "https://github.com/shanedg/trshcmpctr/tree/main/eslint-config-jest#readme",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "echo fake build || true",
    "clean": "rimraf *.build.log",
    "lint": "eslint --max-warnings=0 --fix --cache --cache-location node_modules/.cache/eslint-cache/ .",
    "lint:md": "markdownlint-cli2-config 'node_modules/@trshcmpctr/markdownlint-config/.markdownlint-cli2.jsonc' '**/*.md' '#node_modules'",
    "test": "jest --no-cache --reporters=@trshcmpctr/jest-stdout-reporter --silent"
  },
  "devDependencies": {
    "@trshcmpctr/eslint-config": "workspace:*",
    "@trshcmpctr/jest-stdout-reporter": "workspace:*",
    "@trshcmpctr/markdownlint-config": "workspace:*",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.25.4",
    "eslint-plugin-jest": "~26.1.1",
    "eslint-plugin-node": "~11.1.0",
    "eslint": "^8.7.0",
    "jest": "^27.4.7",
    "markdownlint-cli2": "~0.4.0",
    "rimraf": "~3.0.2",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "eslint": "^7.x.x || ^8.x.x",
    "eslint-plugin-jest": "~26.1.1"
  }
}
