{
  "name": "@trshcmpctr/discord",
  "version": "1.0.0",
  "description": "discord",
  "files": [
    "lib",
    "!lib/**/*.test.*",
    "!lib/**/*.ava-test.*"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module",
  "contributors": [
    {
      "email": "shanedgarrity@gmail.com",
      "name": "Shane Garrity"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shanedg/trshcmpctr.git",
    "directory": "discord"
  },
  "homepage": "https://github.com/shanedg/trshcmpctr/tree/main/discord#readme",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "echo fake build || true",
    "check-latest-updates": "npm-check-updates --upgrade --minimal --target latest --packageFile package.json",
    "check-minor-updates": "npm-check-updates --upgrade --minimal --target minor --packageFile package.json",
    "clean": "rimraf *.build.log *.build.error.log",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "debug": "node --experimental-specifier-resolution=node --trace-warnings --inspect-brk lib/index.js",
    "dev": "nodemon --experimental-specifier-resolution=node --trace-warnings --inspect lib/index.js",
    "lint": "eslint --max-warnings=0 .",
    "lint:md": "markdownlint-cli2-config 'node_modules/@trshcmpctr/markdownlint-config/.markdownlint-cli2.jsonc' '**/*.md' '#node_modules'",
    "start": "node --experimental-specifier-resolution=node lib/index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --no-cache --silent",
    "test:cypress": "npm start & wait-on http://localhost:53134 && npm run cy:run",
    "test:debug": "NODE_OPTIONS='--experimental-vm-modules --inspect' jest --no-cache --runInBand",
    "test:ava": "NODE_OPTIONS=--experimental-specifier-resolution=node ava",
    "test:ava:debug": "NODE_OPTIONS='--experimental-specifier-resolution=node' ava debug --break"
  },
  "jest": {
    "reporters": ["@trshcmpctr/jest-stdout-reporter"]
  },
  "ava": {
    "cache": false,
    "files": ["**/*.ava-test.*"]
  },
  "devDependencies": {
    "@babel/core": "~7.18.5",
    "@babel/eslint-parser": "~7.18.2",
    "@babel/plugin-syntax-import-assertions": "~7.17.12",
    "@jest/globals": "~28.1.0",
    "@tophat/eslint-import-resolver-require": "0.1.3",
    "@trshcmpctr/eslint-config-jest": "workspace:*",
    "@trshcmpctr/eslint-config": "workspace:*",
    "@trshcmpctr/jest-stdout-reporter": "workspace:*",
    "@trshcmpctr/markdownlint-config": "workspace:*",
    "ava": "~4.3.0",
    "cypress": "~10.2.0",
    "eslint-plugin-ava": "~13.2.0",
    "eslint-plugin-cypress": "~2.12.1",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jest": "~26.5.3",
    "eslint-plugin-node": "~11.1.0",
    "eslint": "~8.18.0",
    "jest": "~28.1.0",
    "markdownlint-cli2": "~0.4.0",
    "nodemon": "~2.0.16",
    "npm-check-updates": "~14.1.1",
    "rimraf": "~3.0.2",
    "sinon": "~14.0.0",
    "typescript": "~4.7.4",
    "wait-on": "~6.0.1"
  },
  "dependencies": {
    "cookie-session": "~2.0.0",
    "express": "~4.18.1",
    "hbs": "~4.2.0",
    "node-fetch": "~3.2.4",
    "pino-http": "~8.0.1"
  }
}
