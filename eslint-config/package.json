{
  "name": "@trshcmpctr/eslint-config",
  "version": "1.0.0",
  "description": "Base ESLint config for JavaScript projects.",
  "main": "lib/eslint-config.js",
  "files": ["lib/eslint-config.js"],
  "contributors": [
    {
      "email": "shanedgarrity@gmail.com",
      "name": "Shane Garrity"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shanedg/trshcmpctr.git",
    "directory": "eslint-config"
  },
  "homepage": "https://github.com/shanedg/trshcmpctr/tree/main/eslint-config#readme",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "echo fake build || true",
    "check-latest-updates": "npm-check-updates --upgrade --minimal --target latest --packageFile package.json",
    "check-minor-updates": "npm-check-updates --upgrade --minimal --target minor --packageFile package.json",
    "clean": "rimraf *.build.log *.build.error.log",
    "lint": "eslint --max-warnings=0 --ignore-pattern __fixtures__ .",
    "lint:md": "markdownlint-cli2-config 'node_modules/@trshcmpctr/markdownlint-config/.markdownlint-cli2.jsonc' '**/*.md' '#node_modules'",
    "test": "jest --no-cache --reporters=@trshcmpctr/jest-stdout-reporter --silent"
  },
  "devDependencies": {
    "@trshcmpctr/eslint-config-jest": "workspace:*",
    "@trshcmpctr/jest-stdout-reporter": "workspace:*",
    "@trshcmpctr/markdownlint-config": "workspace:*",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jest": "~26.5.3",
    "eslint-plugin-node": "~11.1.0",
    "eslint": "~8.18.0",
    "jest": "~28.1.0",
    "markdownlint-cli2": "~0.4.0",
    "npm-check-updates": "~14.1.1",
    "rimraf": "~3.0.2",
    "typescript": "~4.7.4"
  },
  "peerDependencies": {
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint": "~8.18.0"
  }
}
