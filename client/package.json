{
  "name": "@trshcmpctr/client",
  "version": "1.0.0",
  "description": "All build config, no substance.",
  "main": "src/index.ts",
  "files": ["dist"],
  "scripts": {
    "build": "webpack --config config/webpack.config.js",
    "build:production": "webpack --config config/webpack.config.js --env production",
    "check-latest-updates": "npm-check-updates --upgrade --minimal --target latest --packageFile package.json",
    "check-minor-updates": "npm-check-updates --upgrade --minimal --target minor --packageFile package.json",
    "clean": "rimraf *.build.log *.build.error.log coverage dist",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --max-warnings=0 .",
    "lint:md": "markdownlint-cli2-config 'node_modules/@trshcmpctr/markdownlint-config/.markdownlint-cli2.jsonc' '**/*.md' '#node_modules'",
    "start": "webpack serve --config config/webpack.config.js",
    "test": "npm run test:jest && npm run type-check",
    "test:cypress": "start-server-and-test start http://localhost:8080 cy:run",
    "test:jest": "jest --config=config/jest/jest.config.js --no-cache --reporters=@trshcmpctr/jest-stdout-reporter --silent",
    "type-check": "tsc",
    "watch": "webpack --config config/webpack.config.js --watch"
  },
  "contributors": [
    {
      "email": "shanedgarrity@gmail.com",
      "name": "Shane Garrity"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shanedg/trshcmpctr.git",
    "directory": "client"
  },
  "homepage": "https://github.com/shanedg/trshcmpctr/tree/main/client#readme",
  "private": true,
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "~7.18.5",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/plugin-transform-modules-commonjs": "~7.18.2",
    "@babel/preset-env": "~7.18.2",
    "@babel/preset-react": "~7.18.6",
    "@babel/preset-typescript": "~7.18.6",
    "@testing-library/jest-dom": "~5.16.4",
    "@testing-library/react": "~13.3.0",
    "@trshcmpctr/eslint-config-jest": "workspace:*",
    "@trshcmpctr/eslint-config-react": "workspace:*",
    "@trshcmpctr/eslint-config-typescript": "workspace:*",
    "@trshcmpctr/eslint-config": "workspace:*",
    "@trshcmpctr/jest-stdout-reporter": "workspace:*",
    "@trshcmpctr/markdownlint-config": "workspace:*",
    "@types/jest": "~28.1.2",
    "@types/node": "~16.11.35",
    "@types/react-dom": "~18.0.5",
    "@types/react": "~18.0.14",
    "@typescript-eslint/eslint-plugin": "~5.30.6",
    "@typescript-eslint/parser": "~5.30.6",
    "babel-jest": "~28.1.0",
    "babel-loader": "~8.2.5",
    "babel-plugin-syntax-dynamic-import": "~6.18.0",
    "css-loader": "~6.7.1",
    "cypress": "10.3.0",
    "eslint-import-resolver-typescript": "~3.2.7",
    "eslint-plugin-cypress": "~2.12.1",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jest": "~26.6.0",
    "eslint-plugin-jsx-a11y": "~6.6.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-react": "~7.30.0",
    "eslint-plugin-testing-library": "~5.5.0",
    "eslint-webpack-plugin": "~3.2.0",
    "eslint": "~8.20.0",
    "html-webpack-plugin": "~5.5.0",
    "jest-environment-jsdom": "~28.1.0",
    "jest-serializer-path": "~0.1.15",
    "jest": "~28.1.0",
    "markdownlint-cli2": "~0.4.0",
    "npm-check-updates": "~15.3.4",
    "rimraf": "~3.0.2",
    "start-server-and-test": "~1.14.0",
    "style-loader": "~3.3.1",
    "terser-webpack-plugin": "~5.3.1",
    "typescript": "~4.7.4",
    "webpack-cli": "~4.10.0",
    "webpack-dev-server": "~4.9.0",
    "webpack": "~5.73.0"
  },
  "dependencies": {
    "core-js": "~3.23.2",
    "react-dom": "~18.2.0",
    "react": "~18.2.0"
  }
}
